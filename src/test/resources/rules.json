[
  {
    "rule_code": "FRAUD_HIGH_VALUE_MULTI_REGION",
    "conditions": [
      {
        "field": "transaction_type",
        "operator": "EQUAL_TO",
        "value": "PAYMENT"
      },
      {
        "field": "amount",
        "operator": "EQUAL_TO",
        "value": 10000
      },
      {
        "field": "region",
        "operator": "IS_ANY_OF",
        "value": ["NA", "EU", "APAC"]
      }
    ],
    "priority": 100,
    "description": "High-value transactions in multiple regions",
    "enabled": true
  },
  {
    "rule_code": "PREMIUM_CUSTOMER_OFFER",
    "conditions": [
      {
        "field": "customer_tier",
        "operator": "IS_ANY_OF",
        "value": ["PREMIUM", "PLATINUM"]
      },
      {
        "field": "account_status",
        "operator": "EQUAL_TO",
        "value": "ACTIVE"
      }
    ],
    "priority": 50,
    "description": "Special offers for premium US customers",
    "enabled": true
  },
  {
    "rule_code": "MULTI_CONDITION_EXPANSION",
    "conditions": [
      {
        "field": "user_type",
        "operator": "IS_ANY_OF",
        "value": ["NEW", "RETURNING"]
      },
      {
        "field": "platform",
        "operator": "IS_ANY_OF",
        "value": ["IOS", "ANDROID"]
      },
      {
        "field": "registration_complete",
        "operator": "EQUAL_TO",
        "value": true
      }
    ],
    "priority": 30,
    "description": "Onboarding flow for new registered users",
    "enabled": true
  },
  {
    "rule_code": "SIMPLE_EQUAL_TO",
    "conditions": [
      {
        "field": "product_type",
        "operator": "EQUAL_TO",
        "value": "ELECTRONICS"
      }
    ],
    "priority": 10,
    "description": "Basic electronics rule",
    "enabled": true
  },
  {
    "rule_code": "DISABLED_RULE",
    "conditions": [
      {
        "field": "customer_tier",
        "operator": "EQUAL_TO",
        "value": "DISABLED"
      }
    ],
    "priority": 1,
    "description": "This rule should not be loaded",
    "enabled": false
  }
]
