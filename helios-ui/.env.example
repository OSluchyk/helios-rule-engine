# Helios UI Environment Configuration
# Copy this file to .env and update the values as needed

# Backend API URL (used in production builds)
# In development, the Vite proxy automatically forwards requests to localhost:8080
# Only change this if deploying to production or using a different backend URL
VITE_API_URL=http://localhost:8080

# Debug mode (enables additional logging in browser console)
# Set to 'true' to enable debug logging
VITE_ENABLE_DEBUG_MODE=false

# Experimental features toggle
# Set to 'true' to enable experimental/preview features
VITE_ENABLE_EXPERIMENTAL_FEATURES=false
